Page{
  background-color: #f5f5f5;
  padding-bottom: 88rpx
}
.order-detail-head{
  padding-bottom: 30rpx;
  text-align: center;
  background-color: #fff
}
.order-detail-head-logo-container{
  padding: 45rpx 0
}
.order-detail-head-logo{
  width: 160rpx;
  height: 160rpx
}
.helpText{
  padding: 8rpx 0;
  font-size: 24rpx;
  color: #999
}
.orderAgainBtn{
  background-color: #f65b25;
  color: #fff;
  font-size: 26rpx;
  width: 208rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  margin-top: 30rpx
}
.order-detail-content{
  margin: 20rpx 0;
  padding: 0 30rpx;
  background-color: #fff

}
.order-detail-content-title{
  padding: 28rpx 0;
  border-bottom: 1rpx solid #eee
}
.order-detail-content-title-logo{
  width: 40rpx;
  height: 40rpx;
  margin-right: 14rpx
}
.order-detail-content-section{
  height: 80rpx;
  border-bottom: 1rpx solid #eee;
  font-size: 28rpx;
  color: #333
}
.order-detail-content-footer{
  padding: 40rpx 0;
  display: flex;
  justify-content: flex-end;
  align-items: baseline
}
.order-detail-content-footer-text1{
  font-size: 28rpx;
  margin-right: 16rpx
}
.bottomContainer{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  top: auto;
  height: 88rpx;
  background-color: #3c3c3c;
  color: #fff;
  padding-left: 20rpx
}
.confirmPay{
  height: 100%;
  min-width: 190rpx;
  padding: 0 10rpx;
  text-align: center;
  background-color: #00d762;
  line-height: 88rpx;
  font-size: 34rpx
}