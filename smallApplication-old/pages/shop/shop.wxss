/**search.wxss**/
page{
  background-color: #fff;
  height: 100%;
  position: relative;
  overflow-y: hidden;
  font-size: 22rpx
}
.noScrollPage{
  height: 100%
}
.shopHeader{
  background-color: #b9b6be;
  color: #fff
}
.shopHeaderMain{
  padding : 20rpx 30rpx 0;
  display: flex
}
.shopHeaderLogo{
  width: 130rpx;
  height: 130rpx;
  border: 1rpx solid #fff;
  border-radius: 4rpx;
  margin-right: 20rpx
}
.shopHeaderDesc{
  width: 540rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around
}
.shopHeaderDescTitle{
  font-size: 36rpx;
  font-weight: 700
}
.afterSplit:after{
  content: "/";
  margin-left: 6rpx;
  margin-right: 6rpx;
  opacity: 0.5
}
.shopHeaderDescMore{
  padding: 10rpx 30rpx
}
.shopHeaderDescMoreSwiper{
  width: 100%;
  height: 32rpx!important;
}
.activityNum{
  min-width: 100rpx
}

.tabContainer{
  line-height: 80rpx;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
  color: #666
}
.tab-item{
  width: 33.33%;
  text-align: center
}
.tab-item-active{
  font-weight: 700;
  color : #f65b25
}
.tab-item-title{
  position: relative;
  display: inline-block
}
.tab-item-active .tab-item-title:after{
  content: '';
  height: 6rpx;
  background-color: #f65b25;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 4rpx
}
.tab-item{
  font-size: 28rpx
}
.tabContentContainer{
  position: absolute;
  top : 288rpx;
  bottom: 96rpx;
  left: 0;
  right: 0;
  height: auto;
  overflow: hidden
}
.productTypeContainer{
  height: 100%;
