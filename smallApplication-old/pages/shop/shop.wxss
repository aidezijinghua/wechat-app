  background-color: #f8f8f8;
}
.productTypeItem{
  padding: 35rpx 15rpx;
  border-bottom: 1rpx solid #eee;
  color: #666;
  font-size: 26rpx
}
.productTypeItemActive{
  background-color: #fff;
  position: relative
}
.productTypeItemActive:before{
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 6rpx;
  background-color: #f65b25;
}
.productTypeItemIcon{
  width: 18rpx;
  height: 25rpx
}
