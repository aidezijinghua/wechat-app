/*自定义模态窗  */
.wxapp-modal{
 width: 100%;
 height: 100%;
 position: fixed;
 top: 0px;
 left: 0px;
 z-index:999;
}
.wxapp-modal .content{
  text-align: center;
  position: absolute;
  top: 50%;
  left: 10%;
  right: 10%;
  transform: translateY(-50%);
  border-radius: 8rpx;
  border: 1px solid #eee;
  z-index: 1000
}
.wxapp-modal .content .header{
 margin: auto;
 padding: 25rpx 60rpx;
 color: #333;
 font-size: 32rpx;
 text-align: center;
 background-color: #FFFFFF;
 position: relative;
 z-index:9999;
 /* border-bottom: 1rpx solid #eee; */
}
.wxapp-modal .content .body{
 margin: auto;
 background-color: #FFFFFF;
 padding: 0 0 40rpx 30rpx;
 z-index:9999;
 overflow: hidden;
}
.wxapp-modal .content .body .bodyTitle{
 color: #666;
 font-size: 26rpx;
 line-height: 40rpx;
 text-align: left;
}
.wxapp-modal .content .footer{
 margin: auto;
 padding: 25rpx 30rpx;
 background-color: #f9f9f9;
 position: relative;
 z-index: 9999;
 text-align: center;
 display: flex;
 align-items: center;
 justify-content: space-between
}
.wxapp-modal .content .footer button{
  padding-left: 25rpx;
  padding-right: 25rpx;
  margin-right: 0;
  background-color: #3199e8;
  color: #fff;
  font-size: 28rpx;
  height: 65rpx;
  line-height: 65rpx
}

.wxapp-modal .content .footer .cancel{
 color: #fa5b43;
 border-right: 1rpx #eee solid;
 border-top-right-radius: 0px;
 border-bottom-right-radius: 0px;
 border-top-left-radius: 8rpx;
 border-bottom-left-radius: 8rpx;
}
.wxapp-modal .content .footer .confirm{
 color: #1ed3fa;
 border-top-left-radius: 0px;
 border-bottom-left-radius: 0rpx;
 border-top-right-radius: 8rpx;
 border-bottom-right-radius: 8rpx;
}
